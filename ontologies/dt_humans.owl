<?xml version="1.0"?>
<rdf:RDF xmlns="https://github.com/RIS-WITH/dt_resources/blob/master/ontologies/dt_humans#"
     xmlns:dt_humans="https://github.com/RIS-WITH/dt_resources/blob/master/ontologies/dt_humans#"
     xmlns:cg_agents="https://github.com/RIS-WITH/common_ground_ontology/blob/master/CG_agents#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="https://github.com/RIS-WITH/dt_resources/blob/master/ontologies/dt_humans">
      <owl:imports rdf:resource="https://github.com/RIS-WITH/common_ground_ontology/blob/master/CG_agents"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    


    <!-- dt_humans#Glove_1 -->

    <owl:NamedIndividual rdf:about="dt_humans#Glove_1">
        <rdf:type rdf:resource="cg_agents#RightHand"/>
        <cg_entities:hasColor rdf:resource="cg_entities#skin_color"/>
    </owl:NamedIndividual>
    


    <!-- dt_humans#Glove_2 -->

    <owl:NamedIndividual rdf:about="dt_humans#Glove_2">
        <rdf:type rdf:resource="cg_agents#LeftHand"/>
        <cg_entities:hasColor rdf:resource="cg_entities#skin_color"/>
    </owl:NamedIndividual>
    


    <!-- dt_humans#Glove_mockRight -->

    <owl:NamedIndividual rdf:about="dt_humans#Glove_mockRight">
        <rdf:type rdf:resource="cg_agents#RightHand"/>
    </owl:NamedIndividual>
    


    <!-- dt_humans#Glove_mockLeft -->

    <owl:NamedIndividual rdf:about="dt_humans#Glove_mockLeft">
        <rdf:type rdf:resource="cg_agents#LeftHand"/>
    </owl:NamedIndividual>
    


    <!-- dt_humans#Helmet_2 -->

    <owl:NamedIndividual rdf:about="dt_humans#Helmet_2">
        <rdf:type rdf:resource="cg_agents#Head"/>
        <cg_entities:hasColor rdf:resource="cg_entities#skin_color"/>
        <cg_sensors:hasSensor rdf:resource="dt_humans#Helmet_2_eye"/>
    </owl:NamedIndividual>



    <!-- dt_humans#Helmet_2_eye -->

    <owl:NamedIndividual rdf:about="dt_humans#Helmet_2_eye">
        <rdf:type rdf:resource="cg_sensors#Sensor"/>
        <cg_sensors:hasFieldOfView rdf:resource="dt_humans#mocap_fov"/>
        <cg_sensors:hasFrameId rdf:resource="dt_humans#Helmet_2_eye_frame"/>
        <cg_sensors:isStatic rdf:datatype="http://www.w3.org/2002/07/owl#bool">false</cg_sensors:isStatic>
    </owl:NamedIndividual>



    <!-- dt_humans#Helmet_2_eye_frame -->

    <owl:NamedIndividual rdf:about="dt_humans#Helmet_2_eye_frame">
        <rdf:type rdf:resource="cg_sensors#FrameId"/>
    </owl:NamedIndividual>
    


    <!-- dt_humans#Helmet_4 -->

    <owl:NamedIndividual rdf:about="dt_humans#Helmet_4">
        <rdf:type rdf:resource="cg_agents#Head"/>
        <cg_entities:hasColor rdf:resource="cg_entities#skin_color"/>
        <cg_sensors:hasSensor rdf:resource="dt_humans#Helmet_4_eye"/>
    </owl:NamedIndividual>


    <!-- dt_humans#Helmet_4_eye -->

    <owl:NamedIndividual rdf:about="dt_humans#Helmet_4_eye">
        <rdf:type rdf:resource="cg_sensors#Sensor"/>
        <cg_sensors:hasFieldOfView rdf:resource="dt_humans#mocap_fov"/>
        <cg_sensors:hasFrameId rdf:resource="dt_humans#Helmet_4_eye_frame"/>
        <cg_sensors:isStatic rdf:datatype="http://www.w3.org/2002/07/owl#bool">false</cg_sensors:isStatic>
    </owl:NamedIndividual>



    <!-- dt_humans#Helmet_4_eye_frame -->

    <owl:NamedIndividual rdf:about="dt_humans#Helmet_4_eye_frame">
        <rdf:type rdf:resource="cg_sensors#FrameId"/>
    </owl:NamedIndividual>
    
    

    <!-- dt_humans#herakles -->

    <owl:NamedIndividual rdf:about="dt_humans#herakles">
        <rdf:type rdf:resource="cg_agents#Head"/>
        <cg_entities:hasColor rdf:resource="cg_entities#skin_color"/>
        <cg_sensors:hasSensor rdf:resource="dt_humans#herakles_eye"/>
    </owl:NamedIndividual>
    


    <!-- dt_humans#herakles_eye -->

    <owl:NamedIndividual rdf:about="dt_humans#herakles_eye">
        <rdf:type rdf:resource="cg_sensors#Sensor"/>
        <cg_sensors:hasFieldOfView rdf:resource="dt_humans#mocap_fov"/>
        <cg_sensors:hasFrameId rdf:resource="dt_humans#herakles_eye_frame"/>
        <cg_sensors:isStatic rdf:datatype="http://www.w3.org/2002/07/owl#bool">false</cg_sensors:isStatic>
    </owl:NamedIndividual>


    
    <!-- dt_humans#herakles_eye_frame -->

    <owl:NamedIndividual rdf:about="dt_humans#herakles_eye_frame">
        <rdf:type rdf:resource="cg_sensors#FrameId"/>
    </owl:NamedIndividual>



    <!-- dt_humans#idiap_head -->

    <owl:NamedIndividual rdf:about="dt_humans#idiap_head">
        <rdf:type rdf:resource="cg_agents#Head"/>
        <cg_entities:hasColor rdf:resource="cg_entities#skin_color"/>
        <cg_sensors:hasSensor rdf:resource="dt_humans#idiap_head_eye"/>
    </owl:NamedIndividual>
    


    <!-- dt_humans#idiap_head_eye -->

    <owl:NamedIndividual rdf:about="dt_humans#idiap_head_eye">
        <rdf:type rdf:resource="cg_sensors#Sensor"/>
        <cg_sensors:hasFieldOfView rdf:resource="dt_humans#mocap_fov"/>
        <cg_sensors:hasFrameId rdf:resource="dt_humans#idiap_head_eye_frame"/>
        <cg_sensors:isStatic rdf:datatype="http://www.w3.org/2002/07/owl#bool">false</cg_sensors:isStatic>
    </owl:NamedIndividual>



    <!-- dt_humans#idiap_head_eye_frame -->

    <owl:NamedIndividual rdf:about="dt_humans#herakles_eye_frame">
        <rdf:type rdf:resource="cg_sensors#FrameId"/>
    </owl:NamedIndividual>



    <!-- dt_humans#mocap_fov -->

    <owl:NamedIndividual rdf:about="dt_humans#mocap_fov">
        <rdf:type rdf:resource="cg_sensors#FieldOfView"/>
        <cg_sensors:fovHasClipNear rdf:datatype="http://www.w3.org/2002/07/owl#real">0.15</cg_sensors:fovHasClipNear>
        <cg_sensors:fovHasClipFar rdf:datatype="http://www.w3.org/2002/07/owl#real">12</cg_sensors:fovHasClipFar>
        <cg_sensors:fovHasHeight rdf:datatype="http://www.w3.org/2002/07/owl#real">70</cg_sensors:fovHasHeight>
        <cg_sensors:fovHasWidth rdf:datatype="http://www.w3.org/2002/07/owl#real">102.4</cg_sensors:fovHasWidth>
    </owl:NamedIndividual>
    


    <!-- dt_humans#human_0 -->
    
    <owl:NamedIndividual rdf:about="dt_humans#human_0">
        <rdf:type rdf:resource="cg_agents#Human"/>
        <cg_agents:hasHead rdf:resource="dt_humans#idiap_head"/>
    </owl:NamedIndividual>
    


    <!-- dt_humans#human_1 -->
    
    <owl:NamedIndividual rdf:about="dt_humans#human_1">
        <rdf:type rdf:resource="cg_agents#Human"/>
        <cg_agents:hasHead rdf:resource="dt_humans#herakles"/>
        <cg_agents:hasLeftHand rdf:resource="dt_humans#Glove_1"/>
        <cg_agents:hasRightHand rdf:resource="dt_humans#Glove_2"/>
    </owl:NamedIndividual>

    <!-- dt_humans#human_2 -->
    
    <owl:NamedIndividual rdf:about="dt_humans#human_2">
        <rdf:type rdf:resource="cg_agents#Human"/>
        <cg_agents:hasHead rdf:resource="dt_humans#Helmet_2"/>
        <cg_agents:hasRightHand rdf:resource="dt_humans#Glove_2"/>
        <cg_agents:hasLeftHand rdf:resource="dt_humans#Glove_mockLeft"/>
    </owl:NamedIndividual>

</rdf:RDF>
